 $ cd db-derby
 $ java -jar lib/derbyrun.jar ij
 > connect 'jdbc:derby:<DatabaseNavn>;create=<true eller false>;';
 >> run '<scriptnavn>.sql';
 >> <queries>
 >> exit;
 > exit;
 
 eksempel:
 $ java -jar lib/derbyrun.jar ij;
 > connect 'jdbc:derby:kalender;create=true';
 >> run 'create_db.sql';
 >> exit;
 > exit;